{
  "servers": {
    "context7": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "--node-options=--experimental-vm-modules",
        "@upstash/context7-mcp@1.0.17"
      ],
      "env": {
        "CONTEXT7_API_KEY": ""
      }
    },
    "sequential-thinking": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking@latest"
      ],
      "env": {
        "DISABLE_THOUGHT_LOGGING": "false"
      }
    },
    "playwright": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@playwright/mcp@latest"
      ],
      "env": {
        "PLAYWRIGHT_HEADLESS": "false"
      }
    },
    "playwright-headed": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@playwright/mcp@latest",
        "--browser",
        "chrome",
        "--viewport-size",
        "1280,720"
      ],
      "env": {}
    }
  }
}
