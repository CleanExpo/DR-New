{
  "mcpServers": {
    "github.com/upstash/context7-mcp": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "--node-options=--experimental-vm-modules", "@upstash/context7-mcp@1.0.6"],
      "env": {
        "CONTEXT7_API_KEY": ""
      }
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/sequentialthinking": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-sequential-thinking"],
      "env": {
        "DISABLE_THOUGHT_LOGGING": "false"
      }
    },
    "sequential-thinking-enhanced": {
      "command": "cmd",
      "args": ["/c", "uvx", "--from", "git+https://github.com/arben-adm/mcp-sequential-thinking", "--with", "portalocker", "mcp-sequential-thinking"],
      "env": {
        "DISABLE_THOUGHT_LOGGING": "false"
      }
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/memory": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-memory"],
      "env": {}
    },
    "github.com/stripe/agent-toolkit": {
      "command": "node",
      "args": ["C:/Users/Disaster Recovery 4/Documents/Cline/MCP/stripe-agent-toolkit-server/dist/index.js"],
      "env": {
        "STRIPE_SECRET_KEY": ""
      }
    },
    "github.com/zcaceres/fetch-mcp": {
      "command": "node",
      "args": ["C:/Users/Disaster Recovery 4/Documents/Cline/MCP/fetch-mcp/dist/index.js"],
      "env": {}
    },
    "context7": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "--node-options=--experimental-vm-modules", "@upstash/context7-mcp@1.0.6"],
      "env": {
        "CONTEXT7_API_KEY": ""
      }
    },
    "taskmaster-ai": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "--package=task-master-ai", "task-master-ai"],
