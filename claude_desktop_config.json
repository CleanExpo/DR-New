    "playwright-isolated": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@playwright/mcp@latest",
        "--isolated",
        "--headless",
        "--browser", "chrome"
      ],
      "env": {}
    }
